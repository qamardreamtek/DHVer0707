{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "heading",
          "name": "pageTitle",
          "size": "H1",
          "text": "Tx Logs"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "icon": "add_circle_outline",
                  "name": "button0",
                  "style": {
                    "marginBottom": "10px"
                  },
                  "text": "Add",
                  "type": "button",
                  "events": {
                    "click": [
                      {
                        "path": "add-tx-log",
                        "type": "popup",
                        "then": [
                          {
                            "code": "await grid0.Reload();",
                            "type": "execute"
                          },
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "events": {
                    "click": [
                      {
                        "condition": "${event?.Value == \"csv\"}",
                        "fileName": "Tx Logs",
                        "name": "MARK10_SQLEXPRESS04.getTxLogs",
                        "parameters": [
                          {
                            "name": "$filter",
                            "value": "${grid0.Query.Filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${grid0.Query.OrderBy}"
                          },
                          {
                            "name": "$expand",
                            "value": ""
                          },
                          {
                            "name": "$select",
                            "value": "WHSE_NO,TX_NO,TX_LINE,STGE_TYPE,STGE_TYPE_TO,STGE_BIN,STGE_BIN_TO,SU_ID,SU_ID_TO,LOC_NO,PLANT,STGE_LOC,SKU_NO,BATCH_NO,STK_CAT,STK_SPECIAL_IND,STK_SPECIAL_NO,GTIN_UNIT,GTIN_QTY,TX_USER,TX_DATE,TX_STS,REMARK,SOURCE,APPROVE_IND,TRN_USER,TRN_DATE,CRT_USER,CRT_DATE"
                          }
                        ],
                        "type": "export"
                      },
                      {
                        "condition": "${event == null || event.Value == \"xlsx\"}",
                        "fileName": "Tx Logs",
                        "format": "xlsx",
                        "name": "MARK10_SQLEXPRESS04.getTxLogs",
                        "parameters": [
                          {
                            "name": "$filter",
                            "value": "${grid0.Query.Filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${grid0.Query.OrderBy}"
                          },
                          {
                            "name": "$expand",
                            "value": ""
                          },
                          {
                            "name": "$select",
                            "value": "WHSE_NO,TX_NO,TX_LINE,STGE_TYPE,STGE_TYPE_TO,STGE_BIN,STGE_BIN_TO,SU_ID,SU_ID_TO,LOC_NO,PLANT,STGE_LOC,SKU_NO,BATCH_NO,STK_CAT,STK_SPECIAL_IND,STK_SPECIAL_NO,GTIN_UNIT,GTIN_QTY,TX_USER,TX_DATE,TX_STS,REMARK,SOURCE,APPROVE_IND,TRN_USER,TRN_DATE,CRT_USER,CRT_DATE"
                          }
                        ],
                        "type": "export"
                      }
                    ]
                  },
                  "icon": "get_app",
                  "items": [
                    {
                      "icon": "",
                      "text": "Excel",
                      "value": "xlsx"
                    },
                    {
                      "icon": "",
                      "text": "CSV",
                      "value": "csv"
                    }
                  ],
                  "name": "splitbutton0",
                  "style": {
                    "marginLeft": "10px",
                    "marginBottom": "10px"
                  },
                  "text": "Export",
                  "type": "splitbutton"
                },
                {
                  "name": "grid0",
                  "allowFiltering": true,
                  "filterMode": "advanced",
                  "allowPaging": true,
                  "allowSorting": true,
                  "data": "${getTxLogsResult}",
                  "events": {
                    "loadData": [],
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "WHSE_NO",
                            "value": "${event.WHSE_NO}"
                          },
                          {
                            "name": "TX_NO",
                            "value": "${event.TX_NO}"
                          },
                          {
                            "name": "TX_LINE",
                            "value": "${event.TX_LINE}"
                          }
                        ],
                        "path": "edit-tx-log",
                        "type": "popup",
                        "then": [
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ]
                      }
                    ]
                  },
                  "title": "",
                  "columns": [
                    {
                      "property": "WHSE_NO",
                      "title": "WHSE NO"
                    },
                    {
                      "property": "TX_NO",
                      "title": "TX NO"
                    },
                    {
                      "property": "TX_LINE",
                      "title": "TX LINE"
                    },
                    {
                      "property": "STGE_TYPE",
                      "title": "STGE TYPE"
                    },
                    {
                      "property": "STGE_TYPE_TO",
                      "title": "STGE TYPE TO"
                    },
                    {
                      "property": "STGE_BIN",
                      "title": "STGE BIN"
                    },
                    {
                      "property": "STGE_BIN_TO",
                      "title": "STGE BIN TO"
                    },
                    {
                      "property": "SU_ID",
                      "title": "SU ID"
                    },
                    {
                      "property": "SU_ID_TO",
                      "title": "SU ID TO"
                    },
                    {
                      "property": "LOC_NO",
                      "title": "LOC NO"
                    },
                    {
                      "property": "PLANT",
                      "title": "PLANT"
                    },
                    {
                      "property": "STGE_LOC",
                      "title": "STGE LOC"
                    },
                    {
                      "property": "SKU_NO",
                      "title": "SKU NO"
                    },
                    {
                      "property": "BATCH_NO",
                      "title": "BATCH NO"
                    },
                    {
                      "property": "STK_CAT",
                      "title": "STK CAT"
                    },
                    {
                      "property": "STK_SPECIAL_IND",
                      "title": "STK SPECIAL IND"
                    },
                    {
                      "property": "STK_SPECIAL_NO",
                      "title": "STK SPECIAL NO"
                    },
                    {
                      "property": "GTIN_UNIT",
                      "title": "GTIN UNIT"
                    },
                    {
                      "property": "GTIN_QTY",
                      "title": "GTIN QTY"
                    },
                    {
                      "property": "TX_USER",
                      "title": "TX USER"
                    },
                    {
                      "property": "TX_DATE",
                      "title": "TX DATE"
                    },
                    {
                      "property": "TX_STS",
                      "title": "TX STS"
                    },
                    {
                      "property": "REMARK",
                      "title": "REMARK"
                    },
                    {
                      "property": "SOURCE",
                      "title": "SOURCE"
                    },
                    {
                      "property": "APPROVE_IND",
                      "title": "APPROVE IND"
                    },
                    {
                      "property": "TRN_USER",
                      "title": "TRN USER"
                    },
                    {
                      "property": "TRN_DATE",
                      "title": "TRN DATE"
                    },
                    {
                      "property": "CRT_USER",
                      "title": "CRT USER"
                    },
                    {
                      "property": "CRT_DATE",
                      "title": "CRT DATE"
                    },
                    {
                      "filterable": false,
                      "sortable": false,
                      "width": "70px",
                      "textAlign": "center",
                      "template": [
                        {
                          "attributes": [
                            {
                              "name": "@onclick:stopPropagation",
                              "value": true
                            }
                          ],
                          "buttonStyle": "danger",
                          "icon": "close",
                          "name": "gridDeleteButton",
                          "text": "",
                          "size": "sm",
                          "type": "button",
                          "events": {
                            "click": [
                              {
                                "condition": "await DialogService.Confirm(\"Are you sure you want to delete this record?\") == true",
                                "error": [
                                  {
                                    "detail": "Unable to delete TxLog",
                                    "severity": "error",
                                    "summary": "Error",
                                    "type": "notify"
                                  }
                                ],
                                "name": "MARK10_SQLEXPRESS04.deleteTxLog",
                                "parameters": [
                                  {
                                    "name": "WHSE_NO",
                                    "value": "${data.WHSE_NO}"
                                  },
                                  {
                                    "name": "TX_NO",
                                    "value": "${data.TX_NO}"
                                  },
                                  {
                                    "name": "TX_LINE",
                                    "value": "${data.TX_LINE}"
                                  }
                                ],
                                "then": [
                                  {
                                    "code": "await grid0.Reload();",
                                    "condition": "result != null",
                                    "type": "execute"
                                  }
                                ],
                                "type": "invoke"
                              }
                            ]
                          }
                        }
                      ]
                    }
                  ],
                  "type": "grid",
                  "itemType": "RadzenDh5.Models.Mark10Sqlexpress04.TxLog"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "MARK10_SQLEXPRESS04.getTxLogs",
        "parameters": [],
        "then": [
          {
            "name": "getTxLogsResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<RadzenDh5.Models.Mark10Sqlexpress04.TxLog>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Tx Logs",
  "access": [
    "Authenticated"
  ],
  "layout": "Main"
}