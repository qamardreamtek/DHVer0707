{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "heading",
          "name": "pageTitle",
          "size": "H1",
          "text": "Group Msts"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "icon": "add_circle_outline",
                  "name": "button0",
                  "style": {
                    "marginBottom": "10px"
                  },
                  "text": "Add",
                  "type": "button",
                  "events": {
                    "click": [
                      {
                        "path": "add-group-mst",
                        "type": "popup",
                        "then": [
                          {
                            "code": "await grid0.Reload();",
                            "type": "execute"
                          },
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "events": {
                    "click": [
                      {
                        "condition": "${event?.Value == \"csv\"}",
                        "fileName": "Group Msts",
                        "name": "MARK10_SQLEXPRESS04.getGroupMsts",
                        "parameters": [
                          {
                            "name": "$filter",
                            "value": "${grid0.Query.Filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${grid0.Query.OrderBy}"
                          },
                          {
                            "name": "$expand",
                            "value": ""
                          },
                          {
                            "name": "$select",
                            "value": "GROUP_ID,GROUP_NAME,OWNER_ID,OWNER_NAME,ENABLE,REMARK,TRN_USER,TRN_DATE,CRT_USER,CRT_DATE"
                          }
                        ],
                        "type": "export"
                      },
                      {
                        "condition": "${event == null || event.Value == \"xlsx\"}",
                        "fileName": "Group Msts",
                        "format": "xlsx",
                        "name": "MARK10_SQLEXPRESS04.getGroupMsts",
                        "parameters": [
                          {
                            "name": "$filter",
                            "value": "${grid0.Query.Filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${grid0.Query.OrderBy}"
                          },
                          {
                            "name": "$expand",
                            "value": ""
                          },
                          {
                            "name": "$select",
                            "value": "GROUP_ID,GROUP_NAME,OWNER_ID,OWNER_NAME,ENABLE,REMARK,TRN_USER,TRN_DATE,CRT_USER,CRT_DATE"
                          }
                        ],
                        "type": "export"
                      }
                    ]
                  },
                  "icon": "get_app",
                  "items": [
                    {
                      "icon": "",
                      "text": "Excel",
                      "value": "xlsx"
                    },
                    {
                      "icon": "",
                      "text": "CSV",
                      "value": "csv"
                    }
                  ],
                  "name": "splitbutton0",
                  "style": {
                    "marginLeft": "10px",
                    "marginBottom": "10px"
                  },
                  "text": "Export",
                  "type": "splitbutton"
                },
                {
                  "name": "grid0",
                  "allowFiltering": true,
                  "filterMode": "advanced",
                  "allowPaging": true,
                  "allowSorting": true,
                  "data": "${getGroupMstsResult}",
                  "events": {
                    "loadData": [],
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "GROUP_ID",
                            "value": "${event.GROUP_ID}"
                          }
                        ],
                        "path": "edit-group-mst",
                        "type": "popup",
                        "then": [
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ]
                      }
                    ]
                  },
                  "title": "",
                  "columns": [
                    {
                      "property": "GROUP_ID",
                      "title": "GROUP ID"
                    },
                    {
                      "property": "GROUP_NAME",
                      "title": "GROUP NAME"
                    },
                    {
                      "property": "OWNER_ID",
                      "title": "OWNER ID"
                    },
                    {
                      "property": "OWNER_NAME",
                      "title": "OWNER NAME"
                    },
                    {
                      "property": "ENABLE",
                      "title": "ENABLE"
                    },
                    {
                      "property": "REMARK",
                      "title": "REMARK"
                    },
                    {
                      "property": "TRN_USER",
                      "title": "TRN USER"
                    },
                    {
                      "property": "TRN_DATE",
                      "title": "TRN DATE"
                    },
                    {
                      "property": "CRT_USER",
                      "title": "CRT USER"
                    },
                    {
                      "property": "CRT_DATE",
                      "title": "CRT DATE"
                    },
                    {
                      "filterable": false,
                      "sortable": false,
                      "width": "70px",
                      "textAlign": "center",
                      "template": [
                        {
                          "attributes": [
                            {
                              "name": "@onclick:stopPropagation",
                              "value": true
                            }
                          ],
                          "buttonStyle": "danger",
                          "icon": "close",
                          "name": "gridDeleteButton",
                          "text": "",
                          "size": "sm",
                          "type": "button",
                          "events": {
                            "click": [
                              {
                                "condition": "await DialogService.Confirm(\"Are you sure you want to delete this record?\") == true",
                                "error": [
                                  {
                                    "detail": "Unable to delete GroupMst",
                                    "severity": "error",
                                    "summary": "Error",
                                    "type": "notify"
                                  }
                                ],
                                "name": "MARK10_SQLEXPRESS04.deleteGroupMst",
                                "parameters": [
                                  {
                                    "name": "GROUP_ID",
                                    "value": "${data.GROUP_ID}"
                                  }
                                ],
                                "then": [
                                  {
                                    "code": "await grid0.Reload();",
                                    "condition": "result != null",
                                    "type": "execute"
                                  }
                                ],
                                "type": "invoke"
                              }
                            ]
                          }
                        }
                      ]
                    }
                  ],
                  "type": "grid",
                  "itemType": "RadzenDh5.Models.Mark10Sqlexpress04.GroupMst"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "MARK10_SQLEXPRESS04.getGroupMsts",
        "parameters": [],
        "then": [
          {
            "name": "getGroupMstsResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<RadzenDh5.Models.Mark10Sqlexpress04.GroupMst>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Group Msts",
  "access": [
    "Authenticated"
  ],
  "layout": "Main"
}