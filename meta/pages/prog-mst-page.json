{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "heading",
          "name": "pageTitle",
          "size": "H1",
          "text": "ProgMstPage"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "icon": "add_circle_outline",
                  "name": "button0",
                  "style": {
                    "marginBottom": "10px"
                  },
                  "text": "Add",
                  "type": "button",
                  "events": {
                    "click": [
                      {
                        "path": "add-prog-mst-page",
                        "type": "popup",
                        "then": [
                          {
                            "code": "await grid0.Reload();",
                            "type": "execute"
                          },
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ]
                      }
                    ]
                  }
                },
                {
                  "name": "grid0",
                  "allowFiltering": true,
                  "filterMode": "advanced",
                  "allowPaging": true,
                  "allowSorting": true,
                  "data": "${getProgMstsResult}",
                  "events": {
                    "loadData": [],
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "PROG_ID",
                            "value": "${event.PROG_ID}"
                          }
                        ],
                        "path": "edit-prog-mst-page",
                        "type": "popup",
                        "then": [
                          {
                            "code": "await InvokeAsync(() => { StateHasChanged(); });",
                            "type": "execute"
                          }
                        ]
                      }
                    ]
                  },
                  "title": "",
                  "columns": [
                    {
                      "property": "PROG_ID",
                      "title": "PROG ID"
                    },
                    {
                      "property": "PROG_NAME",
                      "title": "PROG NAME"
                    },
                    {
                      "property": "PROG_TYPE",
                      "title": "PROG TYPE"
                    },
                    {
                      "property": "PARENT_ID",
                      "title": "PARENT ID"
                    },
                    {
                      "property": "PROG_NODE",
                      "title": "PROG NODE"
                    },
                    {
                      "property": "PROG_PATH",
                      "title": "PROG PATH"
                    },
                    {
                      "property": "PROG_SNO",
                      "title": "PROG SNO"
                    },
                    {
                      "property": "ENABLE",
                      "title": "ENABLE"
                    },
                    {
                      "property": "REMARK",
                      "title": "REMARK"
                    },
                    {
                      "property": "TRN_USER",
                      "title": "TRN USER"
                    },
                    {
                      "property": "TRN_DATE",
                      "title": "TRN DATE"
                    },
                    {
                      "property": "CRT_USER",
                      "title": "CRT USER"
                    },
                    {
                      "property": "CRT_DATE",
                      "title": "CRT DATE"
                    },
                    {
                      "property": "TW_NAME",
                      "title": "TW NAME"
                    },
                    {
                      "property": "CN_NAME",
                      "title": "CN NAME"
                    },
                    {
                      "property": "TH_NAME",
                      "title": "TH NAME"
                    },
                    {
                      "property": "VN_NAME",
                      "title": "VN NAME"
                    },
                    {
                      "filterable": false,
                      "sortable": false,
                      "width": "70px",
                      "textAlign": "center",
                      "template": [
                        {
                          "attributes": [
                            {
                              "name": "@onclick:stopPropagation",
                              "value": true
                            }
                          ],
                          "buttonStyle": "danger",
                          "icon": "close",
                          "name": "gridDeleteButton",
                          "text": "",
                          "size": "sm",
                          "type": "button",
                          "events": {
                            "click": [
                              {
                                "condition": "await DialogService.Confirm(\"Are you sure you want to delete this record?\") == true",
                                "error": [
                                  {
                                    "detail": "Unable to delete ProgMst",
                                    "severity": "error",
                                    "summary": "Error",
                                    "type": "notify"
                                  }
                                ],
                                "name": "MARK10_SQLEXPRESS04.deleteProgMst",
                                "parameters": [
                                  {
                                    "name": "PROG_ID",
                                    "value": "${data.PROG_ID}"
                                  }
                                ],
                                "then": [
                                  {
                                    "code": "await grid0.Reload();",
                                    "condition": "result != null",
                                    "type": "execute"
                                  }
                                ],
                                "type": "invoke"
                              }
                            ]
                          }
                        }
                      ]
                    }
                  ],
                  "type": "grid",
                  "itemType": "RadzenDh5.Models.Mark10Sqlexpress04.ProgMst"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "MARK10_SQLEXPRESS04.getProgMsts",
        "parameters": [],
        "then": [
          {
            "name": "getProgMstsResult",
            "type": "set",
            "value": "${result}",
            "valueType": "IEnumerable<RadzenDh5.Models.Mark10Sqlexpress04.ProgMst>"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "ProgMstPage",
  "access": [
    "P030"
  ],
  "layout": "Main"
}