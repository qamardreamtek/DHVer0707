  "events": {
    "load": [
      {
        "name": "equmodelog",
        "type": "set",
        "value": "new RadzenDh5.Models.Mark10Sqlexpress04.EquModeLog(){}",
        "valueType": "RadzenDh5.Models.Mark10Sqlexpress04.EquModeLog"
      }
    ]
  },
  "name": "Add Equ Mode Log",
  "access": [
    "Authenticated"
  ],
  "layout": "Main"
