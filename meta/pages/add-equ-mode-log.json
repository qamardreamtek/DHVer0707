                            {
                              "type": "button",
                              "buttonType": "submit",
                              "icon": "save",
                              "text": "Save",
                              "buttonStyle": "primary"
                            },
                            {
                              "type": "button",
                              "buttonStyle": "light",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Cancel",
                              "events": {
                                "click": [
                                  {
                                    "type": "close-popup"
                                  }
                                ]
                              }
                            }
                          ]
                        }
                      ]
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to create new EquModeLog!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "MARK10_SQLEXPRESS04.createEquModeLog",
                        "parameters": [
                          {
                            "name": "EquModeLog",
                            "value": "${equmodelog}"
                          }
                        ],
                        "then": [
                          {
                            "type": "close-popup",
                            "result": "${equmodelog}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "data": "${equmodelog}",
                  "visible": "${equmodelog != null}",
                  "itemType": "RadzenDh5.Models.Mark10Sqlexpress04.EquModeLog"
                }
              ]
            }
          ]
        }
      ]
    }
