                          "class": "col-md-3",
                          "components": [
                            {
                              "type": "label",
                              "text": "Total Secs",
                              "component": "TotalSecs",
                              "name": "TotalSecsLabel",
                              "style": {
                                "width": "100%"
                              }
                            }
                          ]
                        },
                        {
                          "type": "col",
                          "class": "col-md-9",
                          "components": [
                            {
                              "valueType": "int?",
                              "type": "numeric",
                              "name": "TotalSecs",
                              "style": {
                                "width": "100%"
                              },
                              "value": "${equmodelog.TotalSecs}"
