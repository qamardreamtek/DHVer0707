@layout MainLayout@inherits RadzenDh5.Pages.R030CoreComponent@using Radzen@using Radzen.Blazor@using RadzenDh5.Models.Mark10Sqlexpress04@inject Microsoft.Extensions.Localization.IStringLocalizer<MainLayout> L<DhAppHeadingAuthMsgOpt IsShowAuth=@IsShowAuth AuthMsg="@AuthMsg" Culture="@Culture" DhUser="@DhUser" DhUsername="@DhUsername" PROG_ID="@PROG_ID" PROG_NAME_BY_CULTURE="@PROG_NAME_BY_CULTURE" progWrt="@progWrt" progMst="@progMst" GoodMsg="@GoodMsg" ErrMsg="@ErrMsg" /><div class="row">    <RadzenButton style="margin-bottom: 10px;margin-left:12px" Text=@Lang("Query") Click="@ButtonQueryClick" />    <RadzenButton style="margin-bottom: 10px;margin-left:12px" Text=@Lang("Refresh") Click="@ButtonRefreshClick" />    <RadzenButton style="margin-bottom: 10px;margin-left:12px" ButtonType="ButtonType.Submit" Disabled="@IsExportDisable" Text=@Lang("Export") ButtonStyle="ButtonStyle.Primary" Click="@ButtonExportClick" /></div><div class="row  d-flex align-items-center " style="margin-bottom:4px">    <div class="col-md-3 ">        <div style="margin-bottom: 4px;" class="row">            <div class="col-md-5  d-flex align-items-center">                <RadzenLabel Text="DATE FROM" style="width: 100%;text-align:right;margin-bottom:4px;">                </RadzenLabel>            </div>            <div class="col-md-7  d-flex align-items-center">                <RadzenDatePicker @bind-Value=@dateFrom DateFormat="yyyy-MM-dd" />            </div>        </div>    </div>    <div class="col-md-3">        <div style="margin-bottom: 4px;" class="row">            <div class="col-md-5  d-flex align-items-center">                <RadzenLabel Text="DATE TO" style="width: 100%;text-align:right;margin-bottom:4px;">                </RadzenLabel>            </div>            <div class="col-md-7">                <RadzenDatePicker @bind-Value=@dateTo DateFormat="yyyy-MM-dd" />            </div>        </div>    </div></div><RadzenGrid  @ref="grid0"            ColumnWidth="125px" AllowColumnResize="true" PagerPosition="PagerPosition.Top"             AllowFiltering="false" FilterMode="FilterMode.Advanced" AllowPaging="true" AllowSorting="false"             Data="@getVr030sResult" TItem="RadzenDh5.Models.Mark10Sqlexpress04.Vr030" @bind-Value="ObjTab0Selected">    <Columns>        <RadzenGridColumn Width="50px" TItem="Vr030" Title="" Filterable="false" Sortable="false" TextAlign="TextAlign.Center">            <Template>                @(getVr030sResult.ToList().IndexOf(context)+1)            </Template>        </RadzenGridColumn>        <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.Vr030" Property="TRN_DATE" Title=@Lang("TRN_DATE")>        </RadzenGridColumn>        <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.Vr030" Property="SKU_NO" Title=@Lang("SKU_NO")>        </RadzenGridColumn>        <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.Vr030" Property="SKU_DESC" Title=@Lang("SKU_DESC")>        </RadzenGridColumn>        <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.Vr030" Property="DATE_CODE" Title=@Lang("DATE_CODE")>        </RadzenGridColumn>        <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.Vr030" Property="EXPIRE_DATE" Title=@Lang("EXPIRE_DATE")>        </RadzenGridColumn>        <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.Vr030" Property="BATCH_NO" Title=@Lang("BATCH_NO")>        </RadzenGridColumn>        <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.Vr030" Property="IN_SNO" Title=@Lang("IN_SNO")>        </RadzenGridColumn>        <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.Vr030" Property="RCV_QTY" Title=@Lang("RCV_QTY")>        </RadzenGridColumn>        <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.Vr030" Property="SKU_UNIT" Title=@Lang("SKU_UNIT")>        </RadzenGridColumn>    </Columns></RadzenGrid>