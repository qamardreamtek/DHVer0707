@*@page "/dialogbasic/{OrderID}"*@

@inject Radzen.DialogService dialogService




<div class="row">
    <div class="col-md-12">
        <h3> @Message</h3>
    </div>
</div>
<div class="row" style="margin-top:16px;">
    <div class="col-md-12" >
        <RadzenButton Click="@((args) => dialogService.Close(true))" Text="OK" Style=" width: 150px" />
    </div>
</div>

@code { 
    [Parameter] public string Message { get; set; }
   

    }
