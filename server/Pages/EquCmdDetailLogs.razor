            <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.EquCmdDetailLog" Property="Cycle2" Title="@L["grid0.Cycle2.Title"]">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.EquCmdDetailLog" Property="Fork2" Title="@L["grid0.Fork2.Title"]">
            </RadzenGridColumn>
