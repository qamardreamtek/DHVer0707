            <RadzenSplitButtonItem Text="@L["splitbutton0.excel.Text"]" Value="xlsx">
            </RadzenSplitButtonItem>
            <RadzenSplitButtonItem Text="@L["splitbutton0.csv.Text"]" Value="csv">
            </RadzenSplitButtonItem>
          </ChildContent>
        </RadzenSplitButton>
        <RadzenGrid @ref="grid0" AllowFiltering="true" FilterMode="FilterMode.Advanced" AllowPaging="true" AllowSorting="true" Data="@getEquCmdDetailLogsResult" TItem="RadzenDh5.Models.Mark10Sqlexpress04.EquCmdDetailLog" RowSelect="@Grid0RowSelect">
          <Columns>
            <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.EquCmdDetailLog" Property="LogDT" Title="@L["grid0.LogDT.Title"]">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.EquCmdDetailLog" Property="CmdSno" Title="@L["grid0.CmdSno.Title"]">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.EquCmdDetailLog" Property="EquNo" Title="@L["grid0.EquNo.Title"]">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.EquCmdDetailLog" Property="CmdMode" Title="@L["grid0.CmdMode.Title"]">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.EquCmdDetailLog" Property="FBank" Title="@L["grid0.FBank.Title"]">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.EquCmdDetailLog" Property="FBayLevel" Title="@L["grid0.FBayLevel.Title"]">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.EquCmdDetailLog" Property="TBank" Title="@L["grid0.TBank.Title"]">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.EquCmdDetailLog" Property="TBayLevel" Title="@L["grid0.TBayLevel.Title"]">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.EquCmdDetailLog" Property="TransferInfo" Title="@L["grid0.TransferInfo.Title"]">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.EquCmdDetailLog" Property="WriteBuffer" Title="@L["grid0.WriteBuffer.Title"]">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.EquCmdDetailLog" Property="WritePLC" Title="@L["grid0.WritePLC.Title"]">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.EquCmdDetailLog" Property="StartAction" Title="@L["grid0.StartAction.Title"]">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.EquCmdDetailLog" Property="Cycle1" Title="@L["grid0.Cycle1.Title"]">
            </RadzenGridColumn>
            <RadzenGridColumn TItem="RadzenDh5.Models.Mark10Sqlexpress04.EquCmdDetailLog" Property="Fork1" Title="@L["grid0.Fork1.Title"]">
