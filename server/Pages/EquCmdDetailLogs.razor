@page "/equ-cmd-detail-logs"
@layout MainLayout
@inherits RadzenDh5.Pages.EquCmdDetailLogsComponent

@using Radzen
@using Radzen.Blazor
@using RadzenDh5.Models.Mark10Sqlexpress04
@using Microsoft.AspNetCore.Identity;
@using RadzenDh5.Models
@using Microsoft.AspNetCore.Authorization
@attribute [Authorize]


@inject Microsoft.Extensions.Localization.IStringLocalizer<EquCmdDetailLogs> L
